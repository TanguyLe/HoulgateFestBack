{
    "name": "houlgatefestback",
    "version": "0.2.0",
    "description": "Back of the website for the HoulgateFest project",
    "license": "MIT",
    "authors": [
        {
            "name": "Nicolas Dejon",
            "email": "dejon.nicolas@gmail.com"
        },
        {
            "name": "Tanguy Le Floch",
            "email": "lefloch.ta@gmail.com"
        }
    ],
    "engines": {
        "node": "17.9.1"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:TanguyLe/HoulgateFestBack.git"
    },
    "main": "index.js",
    "devDependencies": {
        "babel-preset-es2015": "^6.24.1",
        "cross-env": "^7.0.2",
        "nodemon": "^1.14.12",
        "prettier": "^2.0.5"
    },
    "dependencies": {
        "activator": "^3.0.2",
        "async": "^2.6.4",
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.3",
        "babel-polyfill": "^6.26.0",
        "babel-preset-node6": "^11.0.0",
        "bcrypt": "^5.1.0",
        "body-parser": "^1.20.2",
        "cors": "^2.8.4",
        "env-cmd": "^10.1.0",
        "express": "^4.18.2",
        "jsonwebtoken": "^9.0.0",
        "mongoose": "^5.13.16",
        "nodemailer": "^6.9.1"
    },
    "scripts": {
        "prettier-check": "prettier --check .",
        "prettier-run": "prettier --write .",
        "dev": "env-cmd --no-override -f back-dev-env.env nodemon server.js",
        "devStartShotgun": "cross-env-shell HAS_STARTED=1 npm run dev",
        "start": "env-cmd -f back-prod-env.env --silent node server.js",
        "startTest": "env-cmd -f back-test-env.env --silent node server.js",
        "initDB": "env-cmd --no-override -f back-dev-env.env node ./scripts/initDB.js",
        "createTestRecords": "env-cmd --no-override -f back-test-env.env node scripts/createTestRecords.js",
        "removeTestRecords": "env-cmd --no-override -f back-test-env.env node ./scripts/removeTestRecords.js",
        "removeTestShotguns": "env-cmd --no-override -f back-test-env.env node ./scripts/removeTestRecords.js shotgunOnly"
    }
}
